###########
# Floor 0 #
###########
#Calculating the errors of building ID
actualVSpredicted_KNNfloor0$BUILDINGID <- as.integer(actualVSpredicted_KNNfloor0$BUILDINGID)
actualVSpredicted_KNNfloor0$PredictionKnn_building <- as.integer(actualVSpredicted_KNNfloor0$PredictionKnn_building)
actualVSpredicted_KNNfloor0 <- mutate(actualVSpredicted_KNNfloor0, errorsBUILDINGID = PredictionKnn_building - BUILDINGID)

#Calculating the errors of longitude
actualVSpredicted_KNNfloor0$PredictionKnn_lon <- as.integer(actualVSpredicted_KNNfloor0$PredictionKnn_lon)
actualVSpredicted_KNNfloor0 <- mutate(actualVSpredicted_KNNfloor0, errorsLon = PredictionKnn_lon - LONGITUDE)

#Calculating the errors of latitude
actualVSpredicted_KNNfloor0$PredictionKnn_lat <- as.integer(actualVSpredicted_KNNfloor0$PredictionKnn_lat)
actualVSpredicted_KNNfloor0 <- mutate(actualVSpredicted_KNNfloor0, errorsLat = PredictionKnn_lat - LATITUDE)

#Save updated version 
saveRDS(actualVSpredicted_KNNfloor0, file = "actualVSpredicted_KNNfloor0.rds")

